<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.android.tourguide"
    android:versionCode="1"
    android:versionName="1.0">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/abc_vector_test"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.hbyadav.myapplication.MainActivity"
            android:configChanges="orientation"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.hbyadav.myapplication.Manage"
            android:configChanges="orientation"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.hbyadav.myapplication.Student_home"
            android:configChanges="orientation"
            android:screenOrientation="portrait" />
        <activity android:name="com.hbyadav.myapplication.Profile" />
        <activity
            android:name="com.hbyadav.myapplication.Make_schedule"
            android:configChanges="orientation"
            android:screenOrientation="portrait"></activity>
    </application>
</manifest>